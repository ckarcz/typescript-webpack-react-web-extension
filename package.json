{
  "name": "typescript-webpack-react-web-extension",
  "description": "Typescript React Web Extension",
  "version": "1.0.0",
  "author": "ckarcz",
  "license": "UNLICENSED",
  "keywords": [
    "web extension"
  ],
  "scripts": {
    "start:dev": "npm run clean:dev && cross-env WEBPACK_ENV=development DC_ENV=dev DEV_SRV=true webpack-dev-server --config ./webpack/webpack.config.js --colors",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:prod": "npm run build:prod -- --watch",
    "build:dev": "cross-env WEBPACK_ENV=development webpack --config ./webpack/webpack.config.js --colors --mode=development",
    "build:prod": "cross-env WEBPACK_ENV=production webpack --config ./webpack/webpack.config.js --colors --mode=production",
    "clean": "rimraf ./dist",
    "clean:dev": "rimraf ./dist/dev",
    "clean:prod": "rimraf ./dist/prod",
    "lint": "eslint src webpack --color",
    "lint:fix": "eslint src webpack --fix --color",
    "npm-upgrade": "npm-upgrade"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^0.1.19",
    "colors": "^1.3.2",
    "copy-webpack-plugin": "^4.5.3",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.7.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.4",
    "npm-upgrade": "^1.4.1",
    "pug-html-loader": "^1.1.5",
    "pug-loader": "^2.4.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "style-loader": "^0.23.1",
    "webextension-polyfill": "^0.3.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-merge": "^4.1.4",
    "write-file-webpack-plugin": "^4.4.1"
  },
  "dependencies": {
    "loglevel": "^1.6.1",
    "loglevel-plugin-prefix": "^0.8.4",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  }
}
